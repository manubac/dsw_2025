{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2022", "DOM"],
    "jsx": "react-jsx",
  "moduleResolution": "node",
  "strict": true,
  "skipLibCheck": true,
  "allowJs": true,
  "resolveJsonModule": true,
  "allowSyntheticDefaultImports": true,
  "esModuleInterop": true,
    "types": ["vite/client", "react"]
  },
  "include": ["src"]
}
